{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "skipDefaultLibCheck": true
  }
}
